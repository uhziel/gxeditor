{
    "test": {
        "cnName": "示例",
        "desc": "属性展示了目前支持的各数据类型。",
        "children": [ "commodities", "items" ],
        "attributes": {
            "int": {
                "type": "INT",
                "default": 1,
                "min": 0,
                "max": 100,
                "desc": "描述文本，会显示成tips。"
            },
            "float": {
                "type": "DOUBLE",
                "default": 1.1,
                "min": 0.0,
                "max": 200.0,
                "precision": 0.01,
                "optional": true
            },
            "datetime": {
                "type": "DATETIME",
                "default": "2018-01-01 07:02:01"
            },
            "string": {
                "type": "STRING",
                "default": "abc"
            },
            "enum": {
                "type": "ENUM",
                "enumList": [
                    { "value": 1, "caption": "男" },
                    { "value": 2, "caption": "女" }
                ],
                "default": 1
            },
            "file": {
                "type": "FILE",
                "default": ""
            },
            "sound": {
                "type": "FILE",
                "fileType": "SOUND",
                "default": "",
                "rootDir": "sounds"
            }
        }
    },
    "commodities": {
        "children": [ "commodity" ]
    },
    "commodity": {
        "multi": true,
        "attributes": {
            "item_id": {
                "type": "REF",
                "refPath": "item",
                "default": 1,
                "min": 0
            },
           
            "buy_price": {
                "type": "INT",
                "default": 100,
                "min": 0
            },
            "sell_price": {
                "type": "INT",
                "default": 80,
                "min": 0
            }
        }
    },
    "items": {
        "children": [ "item" ]
    },
    "item": {
        "multi": true,
        "attributes": {
            "id": {
                "type": "INT",
                "default": 1,
                "min": 1
            },
            "name": {
                "cnName": "名字",
                "type": "STRING",
                "default": "未命名"
            },
            "icon": {
                "cnName": "图标",
                "type": "FILE",
                "fileType": "IMAGE",
                "default": "",
                "optional": true,
                "rootDir": "icons"
            },
            "__text__": {
                "cnName": "无意义",
                "type": "INT",
                "default": "11",
                "optional": false
            }
        }
    }
}