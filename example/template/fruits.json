{
    "__include__": [
        "${templateDirPath}/__share__/fruit.json"
    ],
    "fruits": {
        "children": [ "fruit" ]
    },
    "fruit": {
        "children": [ "energy" ],
        "multi": true,
        "optional": true,
        "attributes": {
            "name": {
                "type": "STRING",
                "default": "banana"
            },
            "price": {
                "type": "INT",
                "default": 1,
                "min": 1
            }
        }
    },
    "energy": {
        "attributes": {
            "carbohydrates": {
                "type": "DOUBLE",
                "default": 0.0,
                "precision": 0.01
            },
            "fat": {
                "type": "DOUBLE",
                "default": 0.0,
                "precision": 0.01
            },
            "protein": {
                "type": "DOUBLE",
                "default": 0.0,
                "precision": 0.01
            }
        }
    }
}